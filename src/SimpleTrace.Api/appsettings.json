{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:16685"
      }
    }
  },
  "Tracer": {
    "Enabled": true,
    "TraceEndPoint": "http://localhost:14268/api/traces",
    "DefaultTracerId": "DefaultCT",
    "FlushIntervalSecond": 5,
    "TraceArchiveFolder": "Trace"
  },
  "AllowedHosts": "*"
}
